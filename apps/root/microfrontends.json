{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "single-spa-root-microfrontend": {
      "development": {
        "fallback": "single-spa-root-microfrontend.vercel.app"
      }
    },
    "single-spa-content-microfrontend": {
      "routing": [
        {
          "paths": ["/_content/:path*"]
        }
      ]
    },
    "single-spa-navigation-microfrontend": {
      "routing": [
        {
          "paths": ["/_navigation/:path*"]
        }
      ]
    }
  }
}
